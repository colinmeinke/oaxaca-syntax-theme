{
  "name": "oaxaca-syntax",
  "theme": "syntax",
  "version": "0.0.0",
  "description": "A minimal syntax theme for Atom",
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w"
  },
  "author": {
    "name": "Colin Meinke",
    "email": "hello@colinmeinke.com",
    "url": "colinmeinke.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinmeinke/oaxaca-syntax"
  },
  "bugs": {
    "url": "https://github.com/colinmeinke/oaxaca-syntax/issues"
  },
  "keywords": [
    "minimal",
    "syntax",
    "theme"
  ],
  "license": "ISC",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "commitizen": "^2.4.6",
    "conventional-changelog": "^0.5.3",
    "cz-conventional-changelog": "^1.1.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
